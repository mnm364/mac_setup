export PATH="$MAC_SETUP/bin":$PATH

### BEGIN SETUP MYSQL ###

export PATH="/opt/homebrew/opt/mysql-client/bin":$PATH

# Easy connect to local MySQL server
export MYSQL_USERNAME=root
export MYSQL_HOST=localhost

### END SETUP MYSQL ###

### BEGIN SETUP ANDROID ###

export ANDROID_HOME="/Users/$USER/Library/Android/sdk"
export PATH="$ANDROID_HOME/emulator:$ANDROID_HOME/cmdline-tools/latest/bin:$PATH"
# Add Android Studio `studio` command to PATH
export PATH="/Applications/Android Studio.app/Contents/MacOS":$PATH

### END SETUP ANDROID ###
